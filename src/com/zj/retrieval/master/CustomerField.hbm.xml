<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zj.retrieval.master.CustomerField" table="T_CUSTOMER_FIELD">
		<id name="id" type="java.lang.String">
			<column name="ID" />
			<generator class="uuid" />
		</id>
		<property name="key" column="FD_KEY" type="java.lang.String" />
		<property name="value" column="FD_VALUE" type="java.lang.String" />
		<property name="headerId" column="FD_HEADER_ID" type="java.lang.String" />
	</class>
</hibernate-mapping>