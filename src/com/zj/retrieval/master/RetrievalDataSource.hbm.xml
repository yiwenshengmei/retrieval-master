<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zj.retrieval.master.RetrievalDataSource" table="T_RETRIEVAL_DATA_SOURCE">
		<id name="id" type="java.lang.String">
			<column name="`ID`" />
			<generator class="uuid" />
		</id>
		<many-to-one name="node" column="`NODE_ID`" unique="true" />
		<list name="features" cascade="all-delete-orphan" inverse="false">
			<key column="`RDS_ID`" />
			<list-index column="`FEATURE_INDEX`" />
			<one-to-many class="com.zj.retrieval.master.NodeFeature" />
		</list>
		<one-to-one name="matrix" property-ref="retrievalDataSource" cascade="all-delete-orphan" />
	</class>
</hibernate-mapping>