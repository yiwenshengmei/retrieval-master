<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zj.retrieval.master.NodeAttribute" table="T_NODE_ATTRIBUTE">
		<id name="id" type="java.lang.String">
			<column name="`ID`" />
			<generator class="uuid" />
		</id>
		<property name="key" column="`KEY`" type="java.lang.String" />
		<property name="value" column="`VALUE`" type="java.lang.String" />
		<many-to-one name="node" column="`NODE_ID`" />
	</class>
</hibernate-mapping>