<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zj.retrieval.master.Node" table="T_NODE">
		<id name="id" type="java.lang.String">
			<column name="`ID`" />
			<generator class="uuid" />
		</id>
		<property name="name" column="`NAME`" type="java.lang.String" />
		<property name="englishName" column="`NAME_EN`" type="java.lang.String" />
		<property name="desc" column="`DESC`" type="java.lang.String" />
		<property name="label" column="`LABEL`" type="java.lang.String" />
		<property name="parentId" column="`PARENT_ID`" type="java.lang.String" />
		<property name="uri" column="`URI`" type="java.lang.String" />
		<property name="uriName" column="`URI_NAME`" type="java.lang.String" />
		<bag name="images" cascade="save-update">
			<key column="`NODE_ID`" />
			<one-to-many class="com.zj.retrieval.master.NodeImage" />
		</bag>
		<bag name="attributes" cascade="save-update">
			<key column="`NODE_ID`" />
			<one-to-many class="com.zj.retrieval.master.NodeAttribute" />
		</bag>
		<one-to-one name="retrievalDataSource" property-ref="node" cascade="save-update" />
	</class>
</hibernate-mapping>