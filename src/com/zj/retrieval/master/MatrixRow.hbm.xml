<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zj.retrieval.master.MatrixRow" table="T_MATRIX_ROW">
		<id name="id" type="java.lang.String">
			<column name="`ID`" />
			<generator class="uuid" />
		</id>
		<many-to-one name="matrix" column="`MATRIX_ID`" />
		<list name="items" cascade="all-delete-orphan" inverse="false">
			<key column="`ROW_ID`" />
			<list-index column="`ITEM_INDEX`" />
			<one-to-many class="com.zj.retrieval.master.MatrixItem" />
		</list>
	</class>
</hibernate-mapping>